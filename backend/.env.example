# Django Backend Environment Variables
# Copy this to .env and fill in your values

# Django
DJANGO_SECRET_KEY=your-super-secret-key-change-in-production
DEBUG=True
ALLOWED_HOSTS=localhost,127.0.0.1

# Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-supabase-anon-key
SUPABASE_SERVICE_KEY=your-supabase-service-key

# Blockchain
BLOCKCHAIN_RPC_URL=https://sepolia.infura.io/v3/YOUR_KEY
RELIEF_TOKEN_ADDRESS=0x...
SPENDING_CONTROLLER_ADDRESS=0x...
CAMPAIGN_MANAGER_ADDRESS=0x...
ADMIN_PRIVATE_KEY=your-admin-wallet-private-key

# Redis (for Celery)
REDIS_URL=redis://localhost:6379/0

# CORS
CORS_ALLOWED_ORIGINS=http://localhost:5173,http://localhost:3000
